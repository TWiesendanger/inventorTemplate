<?xml version="1.0" encoding="utf-8" ?>
<nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      autoreload ="true"
      throwExceptions ="true">

	<targets>
		<target name="logfile" xsi:type="File" fileName="${var:logPath}\InventorTemplate_${date:format=yyyyMMdd_HHmmss:cached=true}.log" layout="${longdate}|${level:uppercase=true}|${logger}|${message:withexception=true}" />
		<target name="logconsole" xsi:type="Console" />
	</targets>

	<rules>
		<logger name="*" minlevel="Debug" writeTo="logfile,logconsole" />
	</rules>
</nlog>